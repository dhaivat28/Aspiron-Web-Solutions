/* ---------- dropdown code Starts here ------------------- */


.clea > li > div {
	position: absolute;
	display: block;
	width:550px;
	top:100%;
	border-top:5px solid #8FEF8F;
	left: 57%;
	opacity: 0;
	visibility: hidden;
	overflow: hidden;
	border-radius: 0 0 3px 3px;
	
-webkit-transition: all .3s ease .15s;
	-moz-transition: all .3s ease .15s;
	-o-transition: all .3s ease .15s;
	-ms-transition: all .3s ease .15s;
	transition: all .3s ease .15s; 

	z-index:999999;

	background: #fefefe; /* Old browsers */
	background: -moz-linear-gradient(top,  #fefefe 0%, #eaebec 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#fefefe), color-stop(100%,#eaebec)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #fefefe 0%,#eaebec 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #fefefe 0%,#eaebec 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #fefefe 0%,#eaebec 100%); /* IE10+ */
	background: linear-gradient(to bottom,  #fefefe 0%,#eaebec 100%); /* W3C */
	background-repeat: repeat;
	
	border-bottom-right-radius:8px;
	border-bottom-left-radius:8px;
	
	
-webkit-box-shadow: 5px 5px 5px 0px rgba(0,0,0,0.75);
-moz-box-shadow: 5px 5px 5px 0px rgba(0,0,0,0.75);
box-shadow: 5px 5px 5px 0px rgba(0,0,0,0.75);
}

.clea > li:hover > div {
	opacity: 1;
	visibility: visible;
	overflow: visible;
}

